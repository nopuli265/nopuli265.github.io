package bai1;

public class Simple20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int n, d, g;
n = 10;
d =2;
if (d!= 0 && (n%d)==0)
	System.out.println(d+ "Это фактор " + n);
d=0;
if (d!=0 && (n%d)==0 )
	System.out.println(d +" Это фактор" +n);
if (d!= 0 & (n%d)==0)
	System.out.println(d+ " это фактор "+ n);
	}

}
