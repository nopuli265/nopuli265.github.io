package bai1;

public class Simple18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int ir, i;
double dr, d;
ir = 10/3;
i = 10%3;
dr = 10.0/3;
d= 10%3;
System.out.println(" результаты деления и остаток от деления целочсленных переменных "+ ir + "" +i);
System.out.println(" результаты деления и остаток от деления дробных переменных "+ dr + "" +d);

	}

}
